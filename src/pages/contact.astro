---
import Header from "../components/Header.astro";
import Sidebar from "../components/Sidebar.astro";
---

<!doctype html>
<html>
    <Header title="Personal Projects | Daniel Nolan" />
    <body>
        <div class="content">
            <Sidebar selectedItem="contact" />
            <nav class="main">
                <div class="container">
                    <div class="back-nav-container">
                        <a href="/" class="back-nav-button w-inline-block"
                            ><img
                                src="/assets/back.svg"
                                loading="lazy"
                                alt=""
                                class="back-nav-img"
                            />
                            <div>Back</div></a
                        >
                    </div>
                    <div class="content-container">
                        <h1>Get in Touch</h1>
                        <section class="items-container">
                            <div class="resouce-items">
                                <div class="single-item">
                                    <div
                                        class="project-links project-links-list"
                                    >
                                        <ul role="list">
                                            <li>
                                                <a
                                                    href="https://github.com/dnolan" aria-label="Github" target="_blank" rel="noopener"
                                                    >Github</a>
                                            </li>
                                            <li>
                                                <a
                                                    href="https://www.linkedin.com/in/dnolan/" aria-label="LinkedIn" target="_blank" rel="noopener"
                                                    >LinkedIn</a>
                                            </li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                        </section>
                    </div>
                </div>
            </nav>
        </div>

        <!--[if lte IE 9]><script src="//cdnjs.cloudflare.com/ajax/libs/placeholders/3.0.2/placeholders.min.js"></script><![endif]-->
    </body>
</html>
